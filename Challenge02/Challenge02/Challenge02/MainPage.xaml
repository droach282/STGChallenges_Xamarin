<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Challenge02"
             x:Class="Challenge02.MainPage">

  <StackLayout>
    <Entry x:Name="MoneyEntry" Keyboard="Numeric" Placeholder="Change To Make" />
    <Button x:Name="MakeChange" Clicked="MakeChange_OnClicked" Text="Make Change" />
    <Label x:Name="OutputLabel" />
  </StackLayout>

</ContentPage>